### Script to download METABRIC data

if (!requireNamespace("BiocManager", quietly = TRUE))
    install.packages("BiocManager")
##BiocManager::install("MetaGxBreast")
library(MetaGxBreast)

metabric_data <- MetaGxBreast::loadBreastEsets(loadString = "metabric")[[1]]


expression_data <- exprs(metabric_data)
pam50_labels <- metabric_data$subtype
